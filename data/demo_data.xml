            <field name="approval_state">approved</field>
            <field name="commission_rate">12.0</field>
            <field name="is_published">True</field>
            <field name="description_sale">High-performance laptop with 16GB RAM, 512GB SSD, and Intel Core i7 processor. Perfect for work and entertainment.</field>
        </record>
        
