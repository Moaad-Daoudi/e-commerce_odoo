<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Demo Categories -->
        <record id="demo_category_electronics" model="product.public.category">
            <field name="name">Electronics</field>
            <field name="sequence">1</field>
        </record>
        
        <record id="demo_category_fashion" model="product.public.category">
            <field name="name">Fashion</field>
            <field name="sequence">2</field>
        </record>
        
        <record id="demo_category_home" model="product.public.category">
            <field name="name">Home &amp; Garden</field>
            <field name="sequence">3</field>
        </record>
        
        <record id="demo_category_sports" model="product.public.category">
            <field name="name">Sports &amp; Outdoors</field>
            <field name="sequence">4</field>
        </record>
        
        <record id="demo_category_books" model="product.public.category">
