            <field name="sequence">6</field>
        </record>

        <!-- Demo Vendor Partners -->
        <record id="demo_partner_techstore" model="res.partner">
            <field name="name">TechStore Pro</field>
            <field name="email">contact@techstore.com</field>
            <field name="phone">+1-555-0101</field>
            <field name="street">123 Tech Avenue</field>
            <field name="city">San Francisco</field>
            <field name="zip">94102</field>
            <field name="country_id" ref="base.us"/>
            <field name="is_company">True</field>
        </record>
        
        <record id="demo_partner_fashionhub" model="res.partner">
            <field name="name">Fashion Hub</field>
            <field name="email">info@fashionhub.com</field>
            <field name="phone">+1-555-0202</field>
            <field name="street">456 Style Street</field>
            <field name="city">New York</field>
            <field name="zip">10001</field>
            <field name="country_id" ref="base.us"/>
            <field name="is_company">True</field>
        </record>
        
        <record id="demo_partner_homegoods" model="res.partner">
            <field name="name">Home Goods Express</field>
            <field name="email">sales@homegoods.com</field>
            <field name="phone">+1-555-0303</field>
            <field name="street">789 Home Lane</field>
            <field name="city">Chicago</field>
            <field name="zip">60601</field>
            <field name="country_id" ref="base.us"/>
