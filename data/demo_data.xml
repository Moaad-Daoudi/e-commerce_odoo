            <field name="zip">90001</field>
            <field name="country_id" ref="base.us"/>
            <field name="is_company">True</field>
        </record>

        <!-- Demo Vendors -->
        <record id="demo_vendor_techstore" model="marketplace.vendor">
            <field name="partner_id" ref="demo_partner_techstore"/>
            <field name="shop_name">TechStore Pro</field>
            <field name="shop_url">techstore-pro</field>
            <field name="default_commission_rate">12.0</field>
            <field name="state">active</field>
        </record>
        
        <record id="demo_vendor_fashionhub" model="marketplace.vendor">
            <field name="partner_id" ref="demo_partner_fashionhub"/>
            <field name="shop_name">Fashion Hub</field>
            <field name="shop_url">fashion-hub</field>
            <field name="default_commission_rate">15.0</field>
            <field name="state">active</field>
        </record>
        
        <record id="demo_vendor_homegoods" model="marketplace.vendor">
            <field name="partner_id" ref="demo_partner_homegoods"/>
            <field name="shop_name">Home Goods Express</field>
            <field name="shop_url">home-goods-express</field>
